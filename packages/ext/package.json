{
  "name": "ext",
  "displayName": "Markdown Editor",
  "version": "0.0.3",
  "description": "Markdown Editor for VSCode",
  "publisher": "CJL",
  "author": "CJL <2662442385@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/beixiyo/vsc-md-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/beixiyo/vsc-md-editor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.webp",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*",
    "webview/*",
    "CHANGELOG.md"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "onCustomEditor:gemini.wysiwyg-markdown"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "gemini.wysiwyg-markdown",
        "displayName": "WYSIWYG Markdown Editor",
        "selector": [
          {
            "filenamePattern": "*.md"
          }
        ]
      }
    ],
    "commands": []
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --sourcemap",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm build",
    "publish": "vsce publish --no-dependencies",
    "vsix": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "pnpm publish"
  },
  "dependencies": {
    "config": "workspace:*"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@vscode/vsce": "^3.6.0",
    "reactive-vscode": "^0.2.22",
    "tsup": "^8.5.0"
  }
}
