{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vsc-markdown-studio"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/vsc-markdown-studio/dist/**/*.js"
      ],
      "preLaunchTask": "npm: dev:vsc-markdown-studio"
    }
  ]
}
